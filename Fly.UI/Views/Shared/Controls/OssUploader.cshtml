@{
	IEnumerable<string> photos = this.Model;
}
<div class="oss-uploader">
	<div class="photos">
		@if (photos != null)
		{
			foreach (var key in photos)
			{
				if (string.IsNullOrWhiteSpace(key))
				{
					continue;
				}

				var imagePath = Api<IPhotoApi>.Instance.GetThumbnailUrl("ec", key, 100, 100);

				<div class="photo WU_FILE_Pre" data-key="@key">
					<div class="image">
						<img src="@imagePath" width="100" height="100">
					</div>
					<div class="top"><a class="btn-remove" href="javascript:void(0);"><i class="fa fa-trash"></i></a></div>
				</div>
			}
		}
	</div>
	<div class="image-picker webuploader-container">点击选择照片</div>
	<div class="clearfix"></div>
	<div class="error-msg"></div>
	<div class="clearfix"></div>
</div>
